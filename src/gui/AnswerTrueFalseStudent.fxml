<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.AnswerTrueFalseStudent">
    <children>
        <Label fx:id="label" alignment="TOP_CENTER" layoutX="120.0" layoutY="14.0" prefHeight="190.0" prefWidth="360.0" text="Label" />
        <RadioButton fx:id="radio" layoutX="250.0" layoutY="204.0" mnemonicParsing="false" prefHeight="34.0" prefWidth="114.0" text="True" />
        <Button fx:id="next" layoutX="67.0" layoutY="337.0" mnemonicParsing="false" onAction="#next_click" text="Next" />
        <Label alignment="CENTER" layoutX="14.0" layoutY="14.0" prefHeight="25.0" prefWidth="106.0" text="Remaining Time" />
        <Label fx:id="remaining_Time" alignment="CENTER" layoutX="14.0" layoutY="48.0" prefHeight="25.0" prefWidth="106.0" text=".........." />
        <Label fx:id="remaining_Time1" alignment="CENTER" layoutX="14.0" layoutY="84.0" prefHeight="25.0" prefWidth="106.0" text="Time Question" />
        <Label fx:id="remaining_TimeQ" alignment="CENTER" layoutX="14.0" layoutY="122.0" prefHeight="25.0" prefWidth="106.0" text="..............." />
        <Button fx:id="previous" layoutX="497.0" layoutY="337.0" mnemonicParsing="false" onAction="#previous_click" text="Previous" />
        <Button layoutX="274.0" layoutY="350.0" mnemonicParsing="false" onAction="#finish" text="Finish" />
    </children>
</AnchorPane>
