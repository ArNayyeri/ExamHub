<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.AnswerDescriptiveStudent">
    <children>
        <Label fx:id="label" alignment="TOP_CENTER" layoutX="120.0" layoutY="14.0" prefHeight="190.0" prefWidth="360.0" text="Label" textFill="#5d7c7c">
         <font>
            <Font name="Arial Bold" size="12.0" />
         </font></Label>
        <Button fx:id="next" layoutX="375.0" layoutY="301.0" mnemonicParsing="false" onAction="#next_click" style="-fx-background-color: #ff9900;" text="Next Question" />
        <Label alignment="CENTER" layoutX="14.0" layoutY="14.0" prefHeight="25.0" prefWidth="106.0" text="Remaining Time" textFill="#5d7c7c" />
        <Label fx:id="remaining_Time" alignment="CENTER" layoutX="14.0" layoutY="48.0" prefHeight="25.0" prefWidth="106.0" text=".........." />
        <Label fx:id="remaining_Time1" alignment="CENTER" layoutX="487.0" layoutY="14.0" prefHeight="25.0" prefWidth="106.0" text="Time Question" textFill="#5d7c7c" />
        <Label fx:id="remaining_TimeQ" alignment="CENTER" layoutX="487.0" layoutY="48.0" prefHeight="25.0" prefWidth="106.0" text="..............." />
        <Button fx:id="previous" layoutX="94.0" layoutY="301.0" mnemonicParsing="false" onAction="#previous_click" style="-fx-background-color: #ff9900;" text="Previous Question" />
        <Button layoutX="277.0" layoutY="301.0" mnemonicParsing="false" onAction="#finish" style="-fx-background-color: #ff9900;" text="Finish" />
      <Button layoutX="340.0" layoutY="263.0" mnemonicParsing="false" onAction="#get_answer" style="-fx-background-color: #ff9900;" text="Upload image" />
      <Button layoutX="129.0" layoutY="263.0" mnemonicParsing="false" onAction="#show_answer" style="-fx-background-color: #ff9900;" text="Show uploaded image" />
      <ImageView fitHeight="62.0" fitWidth="200.0" layoutX="200.0" layoutY="332.0">
         <image>
            <Image url="@../Images/ExamHub logo 2.png" />
         </image>
      </ImageView>
    </children>
</AnchorPane>
